

<scrolltop></scrolltop>
      <cookiebanner></cookiebanner>
       <hidedynamic></hidedynamic>
        <autoexternal></autoexternal>
    <inactivetab></inactivetab>
          <faviconcart></faviconcart>
 <stickycart></stickycart> 
 <starrating></starrating>
 <instantsearch></instantsearch>
 <script>
var scrollTop={{ app.metafields.ScrollTopToBottom |json  }};

if(scrollTop == undefined){
    window.Scrolltop ={}
}else{
   window.Scrolltop   = scrollTop.items ;
}
var stickyCart ={{ app.metafields.StickyAddCart |json  }};

if(scrollTop == undefined){
    window.StickyCart ={}
}else{
   window.StickyCart   = stickyCart.items ;
}

var FavIcon ={{ app.metafields.CartFavicon |json  }};

if(FavIcon == undefined){
    window.FavIcon ={}
}else{
   window.FavIcon   = FavIcon.items ;
}

var cartNotice ={{ app.metafields.CartNotice |json  }};

if(cartNotice == undefined){
    window.CartNotice  ={}
}else{
   window.CartNotice   = cartNotice.items ;
}

var InstantSearch ={{ app.metafields.InstantSearch |json  }};

if(InstantSearch == undefined){
   window.InstantSearch  ={}
}else{
   window.InstantSearch  = InstantSearch.items ;
}

var AutoExternalLinks ={{ app.metafields.AutoExternalLinks |json  }};

if(AutoExternalLinks == undefined){
  window.AutoExternalLinks  ={}
}else{
  window.AutoExternalLinks = AutoExternalLinks.items ;
}

var InActiveTab ={{ app.metafields.InactiveTabMessage |json  }};

if(AutoExternalLinks == undefined){
  window.InActiveTab  ={}
}else{
  window.InActiveTab = InActiveTab.items ;
}

var cookieBanner ={{ app.metafields.cookiebanner |json  }};

if(cookieBanner == undefined){
  window.cookieBanner  ={}
}else{
  window.cookieBanner = cookieBanner.items ;
}

var HideDynamicCheckout ={{ app.metafields.HideDynamicCheckout |json  }};

if(HideDynamicCheckout == undefined){
  window.HideDynamicCheckout  ={}
}else{
  window.HideDynamicCheckout = HideDynamicCheckout.items ;
}

var productReviews ={{ app.metafields.ProductReviews |json  }};

if(productReviews == undefined){
  window.productReviews  ={}
}else{
  window.productReviews = productReviews.items ;
}

window.status = "{{ app.metafields.AllInOneStore.status}}"
window.money =  "{{  shop.money_format }}" 

window.shop =  "{{ shop.permanent_domain  }}"


window.currentProduct = {{ product  |  json}}

</script>
  
    

  
{% schema %}
{
 "name": "All in One store",
 "javascript": "bundle.js",
 "target": "body",
  "available_if":"{{ app.metafields.AllInOneStore.status }}",
 "settings":[]
}

{% endschema %}